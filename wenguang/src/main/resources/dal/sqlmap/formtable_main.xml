<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="formtable_main">
	<insert id="insert">
		insert into formtable_main_2(fjInputStream,zwInputStream)
		values(#{fjInputStream},#{zwInputStream })
	</insert>
	
	<select id="getById" resultType="com.harmony.wenguang.dao.dataobject.FormtableMainDO">
		select id,fjInputStream,zwInputStream
		 from formtable_main_2
		where id=#{id}
	</select>
	
</mapper>
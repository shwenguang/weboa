<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="wg_test">
	<insert id="insert">
		insert into wg_test(id,name,remark)
		values(#{id},#{name},#{remark})
	</insert>
	<select id="selectAll" resultType="com.harmony.wenguang.dao.dataobject.WgTestDO">
		select * from wg_test
	</select>
</mapper>
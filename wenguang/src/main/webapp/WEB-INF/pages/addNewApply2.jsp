    <script type="text/javascript">
     function pay() {
            var data = { id: $("#div1").html() };
            var json = '{';
            var applicant = ''; document_name = ''; applicant_organization = ''; profession = ''; document_num = ''; legal_person = ''; commun_address = ''; telphone = ''; email = ''; zipcode = ''; zfxxgk_name = ''; zf_name = ''; zf_no = ''; description = ''; zfxx_style = ''; carrier_form = ''; use_style = ''; specific_use = ''; fee_reduction = ''; sqr_signature = ''; contacts = '';
            applicant = $("#input_grqm").val();
            document_name = $("#input_zjmc").val();
            applicant_organization = $("#input_frqm").val(); profession = $("#input_zy").val(); document_num = $("#input_zjhm").val(); legal_person = $("#input_fddbr").val(); commun_address = $("#input_txdz").val(); telphone = $("#input_lxdh").val(); email = $("#input_email").val(); zipcode = $("#input_yzbm").val(); contacts = $("#input_lxr").val(); zfxxgk_name = $("#input_zfjgmc").val(); zf_name = $("#input_zfmc").val(); zf_no = $("#input_zfwh").val(); description = $("#are_jtms").val();
            if ($('input[name="hqfs_yj"]:checked')) {
                if ($('input[name="hqfs_yj"]:checked')[0]) {
                  zfxx_style = $('input[name="hqfs_yj"]:checked')[0].title;
                }
            }
            if ($('input[name="ztxs_zzwb"]:checked')) {
            if( $('input[name="ztxs_zzwb"]:checked')[0]){
                carrier_form = $('input[name="ztxs_zzwb"]:checked')[0].title;
                }
            }
            var obj = document.getElementsByName('zsyt_sc');
            //取到对象数组后，我们来循环检测它是不是被选中
            for (var i = 0; i < obj.length; i++) {
                if (obj[i].checked) use_style += obj[i].title + ',';  //如果选中，将value添加到变量s中
            }
            specific_use = $("#are_jtyt").val();
            var jtyt_wbh = document.getElementsByName('jtyt_wbh');
            for (var i = 0; i < jtyt_wbh.length; i++) {
                if (jtyt_wbh[i].checked) fee_reduction += jtyt_wbh[i].title + ',';  //如果选中，将value添加到变量s中
            }
            sqr_signature = $("#input_shrgz").val();
            if (applicant != undefined && applicant != '' && applicant.indexOf('请填写') < 0) {
                json = json + '"Applicant":"' + applicant + '"';
            }
            if (document_name != undefined && document_name != '' && document_name.indexOf('请填写') < 0) {
                json = json + ',"Document_Name":"' + document_name + '"';
            }
            if (applicant_organization != undefined && applicant_organization != '' && applicant_organization.indexOf('请填写') < 0) {
                json = json + ',"Applicant_Organization":"' + applicant_organization + '"';
            }
            if (profession != undefined && profession != '' && profession.indexOf('请填写') < 0) {
                json = json + ',"Profession":"' + profession + '"';
            }
            if (document_num != undefined && document_num != '' && document_num.indexOf('请填写') < 0) {
                json = json + ',"Document_Num":"' + document_num + '"';
            }
            if (legal_person != undefined && legal_person != '' && legal_person.indexOf('请填写') < 0) {
                json = json + ',"Legal_Person":"' + legal_person + '"';
            }
            if (commun_address != undefined && commun_address != '' && commun_address.indexOf('请填写') < 0) {
                json = json + ',"Commun_Address":"' + commun_address + '"';
            }
            if (telphone != undefined && telphone != '' && telphone.indexOf('请填写') < 0) {
                json = json + ',"Telphone":"' + telphone + '"';
            }
            if (email != undefined && email != '' && email.indexOf('请填写') < 0) {
                json = json + ',"Email":"' + email + '"';
            }
            if (zipcode != undefined && zipcode != '' && zipcode.indexOf('请填写') < 0) {
                json = json + ',"ZipCode":"' + zipcode + '"';
            }
            if (contacts != undefined && contacts != '' && contacts.indexOf('请填写') < 0) {
                json = json + ',"Contacts":"' + contacts + '"';
            }
            if (zfxxgk_name != undefined && zfxxgk_name != '' && zfxxgk_name.indexOf('请填写') < 0) {
                json = json + ',"Zfxxgk_Name":"' + zfxxgk_name + '"';
            }
            if (zf_name != undefined && zf_name != '' && zf_name.indexOf('请填写') < 0) {
                json = json + ',"Zf_Name":"' + zf_name + '"';
            }
            if (zf_no != undefined && zf_no != '' && zf_no.indexOf('请填写') < 0) {
                json = json + ',"Zf_NO":"' + zf_no + '"';
            }
            if (description != undefined && description != '' && description.indexOf('请填写') < 0) {
                json = json + ',"Description":"' + description + '"';
            }
            if (zfxx_style != undefined && zfxx_style != '' && zfxx_style.indexOf('请填写') < 0) {
                json = json + ',"Zfxx_Style":"' + zfxx_style + '"';
            }
            if (carrier_form != undefined && carrier_form != '' && carrier_form.indexOf('请填写') < 0) {
                json = json + ',"Carrier_Form":"' + carrier_form + '"';
            }
            if (use_style != undefined && use_style != '' && use_style.indexOf('请填写') < 0) {
                json = json + ',"Use_Style":"' + use_style + '"';
            }
            if (specific_use != undefined && specific_use != '' && specific_use.indexOf('请填写') < 0) {
                json = json + ',"Specific_Use":"' + specific_use + '"';
            }
            if (fee_reduction != undefined && fee_reduction != '' && fee_reduction.indexOf('请填写') < 0) {
                json = json + ',"Fee_Reduction":"' + fee_reduction + '"';
            }
            if (sqr_signature != undefined && sqr_signature != '' && sqr_signature.indexOf('请填写') < 0) {
                json = json + ',"Sqr_Signature":"' + sqr_signature + '"';
            }
            json = json + "}";
            $.post("http://newappl.eastday.com/apply/api/Sum/ApplyForm", { jsonstr: json }, function (result) {
               if(result){
                 if (result.Code === '1') {
                    alert(result.Message);
                 }else{
                    alert("提交失败,"+result.Message);
                 }
               }
            });
        }
     
 	function SetCwinHeight() {
		var cwin = document.getElementById("cwin");
		if (document.getElementById) {
			if (cwin && !window.opera) {
				if (cwin.contentDocument
						&& cwin.contentDocument.body.offsetHeight)
					cwin.height = cwin.contentDocument.body.offsetHeight + 100;
				else if (cwin.Document && cwin.Document.body.scrollHeight)
					cwin.height = cwin.Document.body.scrollHeight + 100;
			}
		}
	}
    </script>

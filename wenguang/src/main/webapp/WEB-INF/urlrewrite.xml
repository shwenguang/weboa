<urlrewrite>
 	<rule>
		<from>^/1/(\w+)/(\w+).do</from>
		<to>/html/doc.do?path1=$1&amp;path2=$2</to>
 	</rule> 
 	<rule>
		<from>^/1/(\w+)/(\w+)/(\w+).do</from>
		<to>/html/doc.do?path1=$1&amp;path2=$2&amp;path3=$3</to>
 	</rule> 
 	<rule>
		<from>^/2/(\d+).do</from>
		<to>/html/show.do?id=$1</to>
 	</rule>
 	<rule>
		<from>^/wg/pictures/(.*)</from>
		<to>/documents/get.do?docName=$1</to>
 	</rule>
 	<rule>
		<from>^/wg/documents/(.*)</from>
		<to>/documents/get.do?docName=$1</to>
 	</rule>
 	<rule>
		<from>^/wg/reqid/(.*)</from>
		<to>/documents/get.do?docName=$1</to>
 	</rule>
 	<rule>
		<from>^/wg/docid/(.*)</from>
		<to>/documents/get.do?docid=$1</to>
 	</rule>
</urlrewrite>
